{
  "name": "test-github-features",
  "version": "1.0.0",
  "description": "Repo to test github settings for PRs, CI, merging",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./dist && rm -rf ./node_modules",
    "dev": "./node_modules/.bin/nodemon ./src/index.ts",
    "start": "node ./dist/index.js",
    "test": "vitest run",
    "lint": "rome check src",
    "preinstall": "if [ ! -f \"/is-docker\" ]; then echo \"This script must be run inside our Docker container\" >&2; npm run clean; exit 1; fi"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.27",
    "@types/supertest": "^6.0.2",
    "bcrypt": "^5.1.1",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "express": "^4.19.2",
    "rome": "^12.1.3"
  }
}
